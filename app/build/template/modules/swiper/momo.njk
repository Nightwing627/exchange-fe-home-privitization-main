{% macro boxStyle(slideList, extendPath, title, subTitle) %}<section class=momo-swiper-base style=background-color:#0e1a2e><style>.momo-swiper-base{background:url(https://chaindown-oss.oss-accelerate.aliyuncs.com/upload/20200305013937969.png) no-repeat center center;position:relative;height:650px;width:100%;padding-top:60px;box-sizing:border-box}.momo-swiper-base .topLogo{margin:0 auto;height:60px;width:1200px}.momo-swiper-base .title{font-family:HiraKakuStd-W8;font-size:46px;line-height:70px;margin:20px auto 0 auto;text-align:left;width:1200px;color:#fff}.momo-swiper-base .text{line-height:70px;width:1200px;margin:0 auto 0 auto;color:#fff;font-family:PingFangSC-Semibold;font-size:46px;line-height:70px}.momo-swiper-base .subtitle{font-family:Helvetica-Bold;font-size:16px;color:#6e738e;line-height:18px;text-align:left;width:1200px;margin:30px auto}.momo-swiper-base .goSignBut{width:1200px;margin:0 auto;height:50px}.momo-swiper-base .goSignBut button{height:50px;width:200px;border-radius:25px}.momo-swiper-base .imgBox{position:absolute;left:50%;transform:translateX(-50%);bottom:20px}.momo-swiper-base .imgBox .img-ul{height:160px;margin:0 auto;width:1200px;overflow:hidden;margin-top:48px}.momo-swiper-base .imgBox .img-ul li{float:left;margin-left:20px;margin-bottom:50px;box-shadow:0 2px 6px 0 rgba(0,0,0,.5)}.momo-swiper-base .imgBox .img-ul li:first-child{margin:0}.momo-swiper-base .imgBox .img-ul li:hover{border-width:1px;border-style:solid}.momo-swiper-base .imgBox .img-ul li:hover img{width:278px;height:138px;display:block;cursor:pointer}.momo-swiper-base .imgBox .img-ul li img{width:280px;height:140px;display:block;cursor:pointer}.momo-swiper-base .imgBox .sNav{width:1135px;margin:0 auto 0 auto;text-align:center}.momo-swiper-base .imgBox .img-pages{display:inline-block}.momo-swiper-base .imgBox .img-pages li{float:left;width:8px;height:6px;margin-right:10px;cursor:pointer;transition:all .3s}.momo-swiper-base .imgBox .img-pages li:last-child{margin-right:0}.momo-swiper-base .imgBox .img-pages li .pageBg{width:100%;height:100%;background:#fff;opacity:.3;display:block;border-radius:3px}.momo-swiper-base .imgBox .img-pages .active{width:30px;position:relative}.momo-swiper-base .imgBox .img-pages .active .pageBar{display:block;height:6px;background:#fff;transition:all .1s linear;position:absolute;left:0;top:0;border-radius:3px}.europe-swiper-base{width:100%;height:500px;border-bottom:1px solid #263043;position:relative}.europe-swiper-base .banner-box{height:360px;width:100%;position:relative}.europe-swiper-base .banner-item{position:absolute;width:100%;height:100%;left:0;top:0;background-repeat:no-repeat;background-position:top}.europe-swiper-base .thumb-nav{width:1280px;margin:0 auto;position:relative}.europe-swiper-base .thumb-nav .slideBtn{cursor:pointer;position:absolute;top:28px}.europe-swiper-base .thumb-nav .slideBtn:hover{opacity:.7}.europe-swiper-base .thumb-nav .prevBtn{left:0}.europe-swiper-base .thumb-nav .nextBtn{right:0}.europe-swiper-base .thumb-nav .thumb-box{width:1080px;position:relative;height:100px;margin:0 auto;overflow:hidden}.europe-swiper-base .thumb-nav .thumb-box .thumb-ul{position:absolute;top:0}.europe-swiper-base .thumb-nav .thumb-box .thumb-item{height:100px;float:left;width:270px;position:relative;cursor:pointer}.europe-swiper-base .thumb-nav .thumb-box .thumb-item p{padding:40px 20px 20px;font-size:14px;color:#edf4f8}.europe-swiper-base .thumb-nav .thumb-box .thumb-item .pageBg{width:100%;height:2px;position:absolute;bottom:0;left:0;opacity:.3;display:block}.europe-swiper-base .thumb-nav .thumb-box .active{background:#263043}.europe-swiper-base .thumb-nav .thumb-box .active .pageBg{background:#edf4f8}.europe-swiper-base .thumb-nav .thumb-box .active .pageBar{display:block;height:2px;background:#fff;transition:all .1s linear;position:absolute;left:0;bottom:0}</style><div class=topLogo></div><div class=text>{{ title | safe }}</div><div class=subtitle>{{subTitle}}</div><div class=imgBox>{% for li in slideList %} {% if loop.index0 !== 0 %}<ul class="clearfix img-ul"data-id={{loop.index}} style=display:none>{% for item in li %}<li class=u-8-bd><a href="{{ item.httpUrl }}"><img src="{{ item.imageUrl }}"></a></li>{% endfor %}</ul>{% else %}<ul class="clearfix img-ul"data-id={{loop.index}}>{% for item in li %}<li class=u-8-bd><a href="{{ item.httpUrl }}"><img src="{{ item.imageUrl }}"></a></li>{% endfor %}</ul>{% endif %} {% endfor %}<div class=sNav>{% if slideList.length > 1 %}<ul class="clearfix img-pages"></ul>{% endif %}</div></div><script>(()=>{const{getScript}=window.BlockChainUtils;const extendPath="{{extendPath}}";if(extendPath&&!window.InternationSwiperExtend){getScript(`${window.staticDomain}/home/static/${extendPath}`).then(()=>{class InternationSwiper extends window.InternationSwiperExtend{constructor(){super();this.indexInternationalOpen="{{switch.index_international_open}}";this.internationalHomeWwiperBase=document.querySelector(".momo-swiper-base");this.imgListContainer=this.internationalHomeWwiperBase.querySelector(".imgBox");this.imgList=this.imgListContainer.querySelectorAll("li");this.imgPages=this.internationalHomeWwiperBase.querySelector(".img-pages");this.imgBox=this.imgListContainer.querySelectorAll(".img-ul");this.navList=[];this.imgPagesMap={};this.pageTime=0;this.swiperTime=10000;this.bannerCounter=4;this.page=0;this.timer=null;this.thumbPage=0;this.thumbPage_2=0;this.thumbItemWidth="270";this.thumbLeft=0;this.init()}}new InternationSwiper})}})();</script></section>{% endmacro %}