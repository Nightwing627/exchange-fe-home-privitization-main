<section class=king-recommend><style>.king-recommend{width:1180px;margin:50px auto 0;border:1px solid #e2e9ef;box-shadow:0 4px 12px 0 rgba(0,0,0,.1);border-radius:4px;background:#fff;position:relative}.king-recommend .recommend-item{height:80px;line-height:80px;border-bottom:1px solid #e2e9ef}.king-recommend .recommend-item:hover{background:#fff;box-shadow:0 4px 12px 0 rgba(0,0,0,.1);border-radius:4px;margin:0 -10px;padding:0 10px}.king-recommend .recommend-item .td-item{float:left;font-size:18px}.king-recommend .recommend-item .td-item1{width:90px;text-align:center;color:#828ea1}.king-recommend .recommend-item .td-item2{width:565px;padding-left:0;color:#aebacd}.king-recommend .recommend-item .td-item3{width:130px;color:#31363e}.king-recommend .recommend-item .td-item4{width:100px}.king-recommend .recommend-item .td-item-kline{width:86px;padding-top:27px;margin-right:40px}.king-recommend .recommend-item .td-item-btn{position:relative}.king-recommend .recommend-item .td-item-btn button{background:#3f78ff;border-radius:4px;width:70px;height:40px;line-height:40px;text-align:center;font-size:14px;color:#fff;position:absolute;top:50%;transform:translateY(-50%)}.king-recommend .recommend-item .td-item-btn button:hover{background:#5b8cff}.king-recommend .recommend-item .b-cion{color:#31363e}.king-recommend .recommend-item .symbol-icon{margin-right:18px}.king-recommend .recommend-item .symbol-icon img{vertical-align:middle;width:32px;height:32px}.king-recommend .recommend-item.title-th{height:50px;line-height:50px}.king-recommend .recommend-item.title-th .td-item{font-size:12px;color:#aebacd}.king-recommend .recommend-item.title-th .td-item-kline{padding:0}.king-recommend .recommend-item.title-th:hover{background:#fff;box-shadow:none;border-radius:0;margin:0;padding:0}.king-recommend .more{height:60px;line-height:60px;text-align:center}.king-recommend .more span{font-size:16px;color:#3f78ff;cursor:pointer}.king-recommend .more span:hover{color:#5b8cff}.king-recommend .more:hover{background:#fff;border-radius:4px;margin:0 -10px;padding:0 10px}</style><ul class="clearfix recommend-ul recommend-wrap"><li class="clearfix recommend-item title-th"><div class="td-item td-item1">#</div><div class="td-item td-item2">{{locale.home.market}}</div><div class="td-item td-item3">{{locale.home.close}}</div><div class="td-item td-item4">{{locale.home.applies}}</div><div class="td-item td-item-kline">{{locale.home.chart}}</div><div class="td-item td-item-btn">{{locale.h5Add.tradeList1}}</div></li>{% for item in headerSymbol %} {% if helper.recommend.symbolShow(loop.index0) %}<li class="recommend-item recommend-content"data-coin={{item}}><div class="td-item td-item1">{{loop.index}}</div><div class="td-item td-item2">{% if helper.recommend.symbolIcon(item, coinList)%} <span class=symbol-icon><img alt=""src="{{helper.recommend.symbolIcon(item, coinList)}}"> </span>{% endif %} <span class=b-cion>{{helper.recommend.setSymbol(helper.recommend.getCoinShowName(item, symbolAll), 'q')}}</span> <span class=q-cion>{{helper.recommend.setSymbol(helper.recommend.getCoinShowName(item, symbolAll), 'b')}}</span></div><div class="td-item td-item3 recommendPrice"style=height:80px></div><div class="td-item td-item4 percentage"style=height:80px></div><div class="td-item td-item-kline kline"><div class=echart-box style=height:25px></div></div><div class="td-item td-item-btn clearfix"style=height:80px><button class="common-button common-button-solid u-16-cl u-8-bg"style=height:40px;padding-left:20px;padding-right:20px><div class=common-button-slot>{{locale.otcTrade.buy}}</div></button></div></li>{% endif %} {% endfor %}<li class=more><span>{{locale.home.moreMarket}}</span></ul><script>(()=>{const extendPath="{{sourceMap.recommendExtend}}";const{getScript}=window.BlockChainUtils;const websocketPath="{{sourceMap.webSocket}}";if(extendPath&&!window.RecommendExtend){getScript(`${window.staticDomain}/home/static/${extendPath}`).then(()=>{class Recommend extends window.RecommendExtend{constructor(){super();this.$recommend=document.querySelector(".king-recommend");this.echartsPath="{{sourceMap.staticDomain}}/home/static/js/echarts.min.js";this.liItemWidth=310;this.cusKlineColor="#9CA1C2";this.cusLineWidth="1";this.init();this.bindMoreEvent()}bindMoreEvent(){this.$recommend.querySelector(".more").addEventListener("click",()=>{window.location.href="/trade"},false)}}new Recommend})}if(websocketPath&&!window.selfSocket){window.selfSocket=true;getScript(`${window.staticDomain}/home/static/${websocketPath}`)}})();</script></section>