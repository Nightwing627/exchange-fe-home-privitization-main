<section class="inter-recommend inter-recommend-korea"><style>.inter-recommend.inter-recommend-korea{width:100%;box-sizing:border-box;overflow:hidden}.inter-recommend.inter-recommend-korea .recommend-even{width:360px;padding:0 0;margin-right:60px;box-sizing:border-box;float:left;position:relative;cursor:pointer}.inter-recommend.inter-recommend-korea .recommend-even .bl{height:70px;width:1px;position:absolute;left:0;top:25px}.inter-recommend.inter-recommend-korea .recommend-even:first-child .bl{width:0}.inter-recommend.inter-recommend-korea .c-h{height:98px}.inter-recommend.inter-recommend-korea .symbol{font-size:16px;float:left}.inter-recommend.inter-recommend-korea .symbol .b-cion{font-size:30px;color:#31363e;margin-right:2px}.inter-recommend.inter-recommend-korea .symbol p{font-size:14px;color:#828ea1;margin-top:10px}.inter-recommend.inter-recommend-korea .f-right{float:right}.inter-recommend.inter-recommend-korea .price{font-size:30px;color:#f04a73;text-align:right}.inter-recommend.inter-recommend-korea .volume{font-size:12px;margin-top:10px}.inter-recommend.inter-recommend-korea .rose{font-size:18px;text-align:right;margin-top:10px}.inter-recommend.inter-recommend-korea .kline{border:1px solid #e2e9ef;height:120px;padding-top:30px}.inter-recommend.inter-recommend-korea .bz{padding-top:10px;font-size:12px}.inter-recommend.inter-recommend-korea .bz span{display:inline-block;height:2px;width:8px;vertical-align:middle;margin-right:4px}</style><ul class="clearfix recommend-wrap">{% for item in headerSymbol %} {% if helper.recommend.koreaSymbolShow(loop.index0) %}<li class="recommend-content recommend-even"data-coin={{item}}><div class=c-h><div class=symbol><span class=b-cion>{{helper.recommend.setSymbol(helper.recommend.getCoinShowName(item, symbolAll), 'q')}}</span> <span class=q-cion>{{helper.recommend.setSymbol(helper.recommend.getCoinShowName(item, symbolAll), 'b')}}</span><p>市场指数</div><div class=f-right><div class="price recommendPrice"></div><div class="percentage rose">{{ item.roseVal }}</div></div></div><div class="kline td-item td-item-kline"><div class=echart-box style=height:120px></div></div><p class=bz><span class=bz-color></span> 今天</li>{% endif %} {% endfor %}</ul><script>(()=>{const extendPath="{{sourceMap.recommendExtend}}";const{getScript}=window.BlockChainUtils;const websocketPath="{{sourceMap.webSocket}}";if(extendPath&&!window.RecommendExtend){getScript(`${window.staticDomain}/home/static/${extendPath}`).then(()=>{class Recommend extends window.RecommendExtend{constructor(){super();this.$recommend=document.querySelector(".inter-recommend-korea");this.echartsPath="{{sourceMap.staticDomain}}/home/static/js/echarts.min.js";this.liItemWidth=310;this.init()}cusTomKline(color,coin){this.$recommend.querySelector(`li[data-coin="${coin}"]`).querySelector(".bz-color").style.backgroundColor=color}}new Recommend})}if(websocketPath&&!window.selfSocket){window.selfSocket=true;getScript(`${window.staticDomain}/home/static/${websocketPath}`)}})();</script></section>