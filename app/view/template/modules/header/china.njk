<header class="top-common-header" id="common-header">
	<style>
		.top-common-header {
			height: 114px;
			background-color: #fff;
			padding-left: 68px;
			padding-right: 64px;
			display: flex;
			justify-content: space-between;
			align-items: center;
		}
		.linkList-logo {
			display: block;
			height: 63px;
      width: 290px;
			cursor: pointer;
			position: relative;
			left: -40px;
		}
		.linkList-logo img {
			width: 100%;
			height: 100%;
			vertical-align: middle;
		}
		.header-reg-button {
			width: 156px;
			height: 45px;
			border-radius: 12px;
			background-color: #1269FF;
		}
		.header-reg-button .common-button-slot {
			color: #fff;
			font-size: 16px;
			font-weight: 600;
		}
		.header-login-button {
			width: 156px;
			height: 45px;
			border-radius: 12px;
			border: 1px solid #1269FF;
			background-color: #fff;
			margin-right: 16px;
		}
		.header-login-button .common-button-slot {
			color: #1269FF;
			font-size: 16px;
			font-weight: 600;
		}

		.top-common-header .messageCount {
			position: absolute;
			width: 6px;
			height: 6px;
			background: #f00;
			border-radius: 100px;
			top: 14px;
			left: 32px;
		}

		.top-common-header .common-header-optionList {
            float: right;
        }

        .top-common-header .common-header-optionList .header-isLogin-navEven {
            height: 45px;
            float: left;
            margin-right: 25px;
            cursor: pointer;
            position: relative;
						color: #828ea1;
        }

        .top-common-header .common-header-optionList .header-isLogin-navEven .header-isLogin-navEven-title {
            transition: all 0.3s;
            line-height: 45px;
        }

        .top-common-header .common-header-optionList .header-isLogin-navEven:hover .lang-list {
            display: block;
        }

        .top-common-header .common-header-optionList .header-isLogin-navEven .lang-list {
            display: none;
            position: absolute;
            top: 45px;
            left: -20px;
						z-index: 9;
            padding: 10px 0;
						background-color: #fff;
            box-shadow: 0 3px 4px 1px rgba(0, 0, 0, 0.28);
        }

        .top-common-header .common-header-optionList .header-isLogin-navEven .lang-list li {
            cursor: pointer;
            text-align: left;
            min-width: 160px;
            padding: 0 20px;
            font-size: 12px;
            line-height: 40px;
            height: 40px;
            box-sizing: border-box;
        }

        .top-common-header .common-header-optionList .header-isLogin {
            float: left;
        }

				.top-common-header .common-header-optionList .header-guide {
            float: left;
        }

        .top-common-header .common-header-optionList .header-user {
            float: left;
            height: 45px;
            width: 60px;
            line-height: 45px;
            text-align: center;
            border-left-width: 0px;
            border-left-style: solid;
            position: relative;
						align-items: center;
						justify-content: center;
        }

        .top-common-header .common-header-optionList .header-user:hover .header-user-market,
        .top-common-header .common-header-optionList .header-user:hover .message-list,
        .top-common-header .common-header-optionList .header-user:hover .header-user-appdown,
        .top-common-header .common-header-optionList .header-user:hover .icon-active {
            display: block;
        }

        .top-common-header .common-header-optionList .header-user:hover .icon-info {
            display: none;
        }

        .top-common-header .common-header-optionList .header-user .icon-active {
            display: none;
        }

        .top-common-header .common-header-optionList .header-user .header-user-market {
            padding-bottom: 10px;
        }

        .top-common-header .common-header-optionList .header-user .header-user-market,
        .top-common-header .common-header-optionList .header-user .header-user-appdown,
        .top-common-header .common-header-optionList .header-user .header-user-message {
            box-shadow: 0 3px 4px 1px rgba(0, 0, 0, 0.28);
            top: 45px;
            display: none;
            position: absolute;
            width: 240px;
            height: 120px;
            left: -90px;
            z-index: 9999;
						background-color: #fff;
        }

        .top-common-header .common-header-optionList .header-user .header-user-message {
            height: auto;
            width: 320px;
            right: -99px;
            left: auto;
            min-height: 100px;
        }

        .top-common-header .common-header-optionList .header-user .header-user-message .header-user-text {
            height: auto;
        }

        .top-common-header .common-header-optionList .header-user .header-user-message .header-user-text li {
            height: 30px;
            line-height: 30px;
            overflow: hidden;
        }

        .top-common-header .common-header-optionList .header-user .header-user-message .header-user-text .mesageNav {
            cursor: pointer;
        }

        .top-common-header .common-header-optionList .header-user .header-user-icon {
            cursor: pointer;
        }

        .top-common-header .common-header-optionList .header-user .header-user-text {
            height: 80px;
            padding: 16px 22px;
            box-sizing: border-box;
            line-height: 24px;
            text-align: left;
            font-size: 12px;
        }

        .top-common-header .common-header-optionList .header-user .header-user-text .userText {
            font-size: 14px;
            cursor: pointer;
        }

        .top-common-header .common-header-optionList .header-user .header-user-out {
            height: 40px;
            padding: 0 22px;
            line-height: 40px;
            text-align: left;
            font-size: 12px;
            cursor: pointer;
						background-color: #fff;
        }

        .top-common-header .common-header-optionList .header-user .header-user-appdown {
            box-sizing: border-box;
            left: auto;
            right: 0;
            height: 177px;
            width: 340px;
            padding: 30px 40px;
        }

        .top-common-header .common-header-optionList .header-user .header-user-appdown .code-box {
            width: 90px;
            float: left;
        }

        .top-common-header .common-header-optionList .header-user .header-user-appdown .code-box .download-options-code {
            width: 90px;
            height: 90px;
            overflow: hidden;
        }

        .top-common-header .common-header-optionList .header-user .header-user-appdown .code-box .download-options-code img {
            width: 110px;
            position: relative;
            top: -10px;
            left: -10px;
        }

        .top-common-header .common-header-optionList .header-user .header-user-appdown .code-box .download-options-codeText {
            font-family: PingFangSC-Regular;
            font-size: 12px;
            line-height: 12px;
            color: #828ea1;
            letter-spacing: 0;
            text-align: center;
            margin-top: 10px;
        }

        .top-common-header .common-header-optionList .header-user .header-user-appdown .download-options-buttons {
            float: right;
        }

        .top-common-header .common-header-optionList .header-user .header-user-appdown .download-options-buttons button {
            display: block;
            width: 130px;
        }

        .top-common-header .common-header-optionList .header-user .header-user-appdown .download-options-buttons button:first-child {
            margin-bottom: 10px;
        }

        .top-common-header .common-header-optionList .header-lang {
            float: left;
            height: 45px;
            min-width: 120px;
            line-height: 45px;
            text-align: center;
            font-size: 14px;
            position: relative;
            user-select: none;
        }

        .top-common-header .common-header-optionList .header-lang .show-lang {
            cursor: pointer;
            padding: 0 20px;
            font-size: 12px;
            box-sizing: border-box;
        }

        .top-common-header .common-header-optionList .header-lang:hover .lang-list {
            display: block;
        }

        .top-common-header .common-header-optionList .header-lang .lang-list {
            display: none;
            position: absolute;
            top: 45px;
            right: 0px;
						z-index: 9;
            padding: 10px 0;
						background-color: #fff;
            box-shadow: 0 3px 4px 1px rgba(0, 0, 0, 0.28);
        }

        .top-common-header .common-header-optionList .header-lang .lang-list li {
            cursor: pointer;
            text-align: left;
            min-width: 160px;
            padding: 0 20px;
            font-size: 12px;
            line-height: 40px;
            height: 40px;
            box-sizing: border-box;
        }
			.header-isLogin-navEven .lang-list li.h-4-bg, .header-lang .lang-list li.h-4-bg {
					background-color: rgba(167, 180, 203, .1);
				}
	</style>

	<!-- logo -->
	<a class="linkList-logo" href="{{ headerLink.home }}">
		<img src="{{ sourceMap.knox }}"/>
	</a>

	<!-- sign up -->
	<div class="common-header-optionList">
	
		<!-- 未登录时 -->
		<div class="header-guide unlogin-set">
			<button class="header-login-button">
					<div class="common-button-slot">
							{{ locale.header.login }}
					</div>
			</button>
			<button class="header-reg-button">
				<div class="common-button-slot">
					{{ locale.header.register }}
				</div>
			</button>
		</div>

		<div class="header-isLogin login-set" style="display: none">
			<!-- 资产 -->
			<div class="header-isLogin-navEven">
					<div class="header-isLogin-navEven-title">
							{{ locale.header.assets }}
					</div>
					<ul class="lang-list h-1-bg">
							{% for item in assetsList %}
							<li
											class="b-2-cl"
											data-link="{{item.link}}"
							>{{ item.title }}
							</li>
							{% endfor %}
					</ul>
			</div>
			<!-- 订单 -->
			<div class="header-isLogin-navEven">
					<div class="header-isLogin-navEven-title">
							{{ locale.header.order }}
					</div>
					<ul class="lang-list h-1-bg">
							<!--  || activeMark === item.link -->
							{% for item in orderList %}
							<li
											class="b-2-cl"
											data-link="{{item.link}}"
							>{{ item.title }}
							</li>
							{% endfor %}
					</ul>
			</div>
		</div>
		<!-- 设置 -->
		{% if colorList.length %}
		<div class="header-user h-2-bd" id="color-set">
				<div class="header-user-icon icon-info">
						<svg class="icon icon-18" aria-hidden="true">
								<use xlink:href="#icon-b_2"></use>
						</svg>
				</div>
				<div class="header-user-icon icon-active">
						<svg class="icon icon-18" aria-hidden="true">
								<use xlink:href="#icon-b_2_1"></use>
						</svg>
				</div>
		</div>
		{% endif %}
		<!-- 消息中心 -->
		<div class="header-user h-2-bd login-set" style="display: none">
				<div class="messageCount" id="messageMore" style="display: none"></div>
				<div class="header-user-icon icon-info goMessage">
						<svg class="icon icon-18" aria-hidden="true">
								<use xlink:href="#icon-b_3"></use>
						</svg>
				</div>
				<div class="header-user-icon icon-active goMessage">
						<svg class="icon icon-18" aria-hidden="true">
								<use xlink:href="#icon-b_3_1"></use>
						</svg>
				</div>
				<div class="header-user-message h-1-bg goMessage">
						<ul class="header-user-text">
						</ul>
						<div class="header-user-out h-4-bg x-3-cl">
								{{ locale.header.more }}
						</div>
				</div>
		</div>
		<!-- 个人中心 -->
		<div class="header-user h-2-bd login-set" style="display: none">
				<div class="header-user-icon icon-info goPersonal">
						<svg class="icon icon-18" aria-hidden="true">
								<use xlink:href="#icon-b_4"></use>
						</svg>
				</div>
				<div class="header-user-icon icon-active goPersonal">
						<svg class="icon icon-18" aria-hidden="true">
								<use xlink:href="#icon-b_4_1"></use>
						</svg>
				</div>
				<div class="header-user-market h-1-bg">
						<ul class="header-user-text">
								<li class="x-3-cl userText goPersonal">
								</li>
								<li class="x-2-cl userStatus">
								</li>
						</ul>
						<div class="header-user-out h-4-bg x-3-cl logout">
								<!-- 退出 -->
								{{ locale.header.out }}
						</div>
				</div>
		</div>
		<div class="h-2-bd header-lang x-2-cl">
				<div class="show-lang">
						Language
						<svg class="icon icon-14" aria-hidden="true">
								<use xlink:href="#icon-a_13"></use>
						</svg>
				</div>
				<ul class="lang-list h-1-bg b-2-cl">
						{% for item in lanList %}
								{% if currenLan === item.id %}
										<li data-key="/{{ item.id }}" class="h-4-bg x-3-cl">
												{{ item.name}}
										</li>
								{% else %}
										<li data-key="/{{ item.id }}">
												{{ item.name}}
										</li>
								{% endif %}
						{% endfor %}
				</ul>
		</div>

	</div>

	<ul id="colorList" style="display: none">
			{% for item in colorList %}
			<li data-checked="{{ item.checked }}" data-id="{{ item.skinId }}">
					<section class="common-redio">
							<svg aria-hidden="true" class="icon icon-16 uncheck">
									<use xlink:href="#icon-c_14"></use>
							</svg>
							<svg aria-hidden="true" class="icon icon-16 check">
									<use xlink:href="#icon-c_14_1"></use>
							</svg>
					</section>
					<span class="b-1-cl">{{ item.mainClor }}</span>
			</li>
			{% endfor %}
	</ul>

	<script>
			(function () {
					const {getScript} = window.BlockChainUtils;
					const extendPath = "{{sourceMap.headerExtend}}";
					if (extendPath && !window.HeaderExtend) {
							getScript(`${window.staticDomain}/home/static/${extendPath}`).then(() => {
									class Header extends window.HeaderExtend {
											constructor() {
													super();
													this.dialogPath = "{{sourceMap.dialog}}";
													this.etfOpen = Number("{{switch.etfOpen}}");
													this.locale = {
															titleText: '{{locale.header.set}}',
															closeTextProps: '{{locale.components.dialog.closeText}}',
															confirmTextProps: '{{locale.components.dialog.confirmText}}',
															color: '{{locale.header.color}}',
															userStatus: '{{locale.header.userStatus}}',
													};
													this.userState = {
															0: "{{locale.header.userStatus1}}",
															1: "{{locale.header.userStatus2}}",
															2: "{{locale.header.userStatus3}}"
													};
													this.init();
											}
									}

									new Header();
							});
					}

			})();
	</script>
</header>