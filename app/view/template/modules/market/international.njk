<div class="c-2-bg inter-tableContentBase international-market-container" id="table-content">
    <style>
        .home-table {
            min-width: 1100px;
            width: 100%;
            font-size: 12px;
        }

        .home-table .home-loading {
            height: 300px;
            position: relative;
        }

        .home-table .home-loading .loading-page {
            position: absolute;
            left: 50%;
            top: 50%;
            transform: translate3d(-50%, -50%, 0);
        }

        .home-table .home-nodata {
            height: 300px;
            position: relative;
            padding: 80px 0 80px;
            text-align: center;
            font-size: 12px;
            box-sizing: border-box;
        }

        .home-table .home-nodata p {
            margin-top: 5px;
        }

        .home-table .home-nodata .icon {
            font-size: 36px;
        }

        .home-table .home-thead {
            user-select: none;
            width: 100%;
            font-size: 12px;
            border-bottom-width: 1px;
            border-bottom-style: solid;
            height: 30px;
            padding: 0 20px;
            box-sizing: border-box;
        }

        .home-table .home-thead .sortable {
            cursor: pointer;
        }

        .home-table .home-thead li {
            height: 30px;
            line-height: 30px;
            text-align: right;
            float: left;
        }

        .home-table .home-thead li:nth-child(1) {
            width: 12%;
        }

        .home-table .home-thead li:nth-child(2) {
            width: 15%;
        }

        .home-table .home-thead li:nth-child(3) {
            width: 13%;
        }

        .home-table .home-thead li:nth-child(4) {
            width: 15%;
        }

        .home-table .home-thead li:nth-child(5) {
            width: 15%;
        }

        .home-table .home-thead li:nth-child(6) {
            width: 15%;
        }

        .home-table .home-thead li:nth-child(7) {
            width: 15%;
        }

        .home-table .home-thead li:first-child {
            text-align: left;
        }

        .home-table .home-tbody-box {
            transition: all 0.5s;
            overflow: hidden;
            position: relative;
        }

        .home-table .home-tbody .home-tbody-title {
            height: 55px;
            border-bottom-width: 1px;
            border-bottom-style: solid;
            line-height: 55px;
        }

        .home-table .home-tbody .home-tbody-title .lable {
            border-left-width: 3px;
            border-left-style: solid;
            padding-left: 10px;
            margin-left: 1px;
        }

        .home-table .home-tbody .home-tbody-list .home-tbody-li {
            height: 55px;
            border-bottom-width: 1px;
            border-bottom-style: solid;
            padding: 0 20px;
            overflow: hidden;
        }

        .home-table .home-tbody .home-tbody-list .home-tbody-li .even {
            line-height: 55px;
            float: left;
            text-align: right;
        }

        .home-table .home-tbody .home-tbody-list .home-tbody-li .even:nth-child(1) {
            width: 12%;
        }

        .home-table .home-tbody .home-tbody-list .home-tbody-li .even:nth-child(2) {
            width: 15%;
        }

        .home-table .home-tbody .home-tbody-list .home-tbody-li .even:nth-child(3) {
            width: 13%;
        }

        .home-table .home-tbody .home-tbody-list .home-tbody-li .even:nth-child(3):after {
            content: '';
            display: inline-block;
            height: 100%;
            vertical-align: middle;
        }

        .home-table .home-tbody .home-tbody-list .home-tbody-li .even:nth-child(4) {
            width: 15%;
        }

        .home-table .home-tbody .home-tbody-list .home-tbody-li .even:nth-child(5) {
            width: 15%;
        }

        .home-table .home-tbody .home-tbody-list .home-tbody-li .even:nth-child(6) {
            width: 15%;
        }

        .home-table .home-tbody .home-tbody-list .home-tbody-li .even:nth-child(7) {
            width: 15%;
        }

        .home-table .home-tbody .home-tbody-list .home-tbody-li .even .rose-label {
            display: inline-block;
            width: 70px;
            height: 24px;
            text-align: center;
            line-height: 24px;
            padding: 0;
        }

        .home-table .home-tbody .home-tbody-list .home-tbody-li .even .evenSymbol {
            margin-left: 22px;
            font-weight: 700 !important;
            cursor: pointer;
            font-size: 14px;
            position: relative;
        }

        .home-table .home-tbody .home-tbody-list .home-tbody-li .even .evenSymbol .coin-label {
            position: absolute;
            right: 0;
            transform: translateX(100%);
            top: 0;
            white-space: nowrap;
            position: absolute;
            right: -5px;
            transform: translateX(100%);
            top: -4px;
            white-space: nowrap;
            display: inline-block;
            font-size: 12px;
            font-weight: 400;
            padding: 0px 4px;
            border-radius: 2px;
            line-height: 17px;
            overflow: hidden;
        }

        .home-table .home-tbody .home-tbody-list .home-tbody-li .even .evenSymbol .coin-label .coin-label {
            position: relative;
            z-index: 1;
            margin: 0;
            padding: 0;
        }

        .home-table .home-tbody .home-tbody-list .home-tbody-li .even .evenSymbol .coin-label .coin-bg {
            display: inline-block;
            width: 100%;
            height: 100%;
            position: absolute;
            left: 0;
            top: 0;
            opacity: 0.2;
        }

        .home-table .home-tbody .home-tbody-list .home-tbody-li .even .marketIcon {
            cursor: pointer;
        }

        .home-table .home-tbody .home-tbody-list .home-tbody-li .newPrice {
            line-height: auto !important;
            padding-top: 14px;
            box-sizing: border-box;
        }

        .home-table .home-tbody .home-tbody-list .home-tbody-li .newPrice p {
            line-height: 15px;
        }

        .home-table .home-tbody .home-tbody-list .home-tbody-li .newPrice .price {
            font-size: 14px;
        }

        .home-table .home-tbody .home-tbody-list .home-tbody-li .even:first-child {
            text-align: left;
            position: relative;
        }

        .tableContent {
            width: 1180px;
            margin: 50px auto 0 auto;
            position: relative;
            box-shadow: 0px -2px 10px rgba(0, 0, 0, 0.2);
        }

        .tableContent .common-table {
            background: none;
        }

        .tableContent .market-title-box {
            border-bottom-width: 1px;
            border-bottom-style: solid;
            height: 55px;
            line-height: 53px;
            padding-right: 320px;
            padding-left: 20px;
            border-radius: 2px 2px 0 0;
        }

        .tableContent .market-title {
            display: flex;
            text-align: center;
        }

        .tableContent .market-title li {
            float: left;
            height: 53px;
            line-height: 55px;
            cursor: pointer;
            user-select: none;
            flex: 1;
        }

        .tableContent .market-title li.selected {
            border-bottom-width: 2px;
            border-bottom-style: solid;
        }

        .tableContent .search {
            position: absolute;
            top: 12px;
            right: 20px;
            height: 28px;
            line-height: 26px;
            width: 200px;
            border-radius: 2px;
            border-width: 1px;
            border-style: solid;
        }

        .tableContent .search .icon-serach {
            margin: 0 10px;
            display: inline-block;
        }

        .tableContent .search .search-input {
            line-height: 26px;
            border: none;
            width: 165px;
        }

        .tableContent .search input::-webkit-input-placeholder {
            color: #515a6b;
        }

        .tableContent .table-body td .rose-label {
            display: inline-block;
            width: 70px;
            height: 24px;
            text-align: center;
            line-height: 24px;
            padding: 0;
        }

        .tableContent .table-body td .symbolName,
        .tableContent .table-body td .icon-btn {
            padding: 0;
            vertical-align: inherit;
            margin: 0;
            display: inline-block;
        }

        .tableContent .table-body td .symbolName {
            width: 80%;
            margin-left: 10px;
        }

        .tableContent .fontSize14 {
            font-size: 14px;
        }
        .international-market-container.inter-tableContentBase {
            margin: 0 110px 0 110px;
            position: relative;
        }

        .international-market-container.inter-tableContentBase .common-table {
            background: none;
        }

        .international-market-container.inter-tableContentBase .market-title-box {
            border-bottom-width: 1px;
            border-bottom-style: solid;
            height: 55px;
            line-height: 53px;
            border-radius: 2px 2px 0 0;
        }

        .international-market-container.inter-tableContentBase .market-title {
            text-align: center;
        }

        .international-market-container.inter-tableContentBase .market-title li {
            float: left;
            height: 53px;
            line-height: 55px;
            cursor: pointer;
            user-select: none;
            padding: 0 25px;
        }

        .international-market-container.inter-tableContentBase .market-title li.selected {
            border-bottom-width: 2px;
            border-bottom-style: solid;
        }

        .international-market-container.inter-tableContentBase .search {
            position: absolute;
            top: 12px;
            right: 0;
            height: 28px;
            line-height: 26px;
            width: 200px;
            border-radius: 2px;
        }

        .international-market-container.inter-tableContentBase .search .icon-serach {
            margin: 0 10px;
        }

        .international-market-container.inter-tableContentBase .search .search-input {
            line-height: 26px;
            border: none;
            width: 162px;
        }

        .international-market-container.inter-tableContentBase .search input::-webkit-input-placeholder {
            color: #515a6b;
        }

        .international-market-container.inter-tableContentBase .table-body td .rose-label {
            display: inline-block;
            width: 70px;
            height: 24px;
            text-align: center;
            line-height: 24px;
            padding: 0;
        }

        .international-market-container.inter-tableContentBase .table-body td .symbolName,
        .international-market-container.inter-tableContentBase .table-body td .icon-btn {
            padding: 0;
            vertical-align: inherit;
            margin: 0;
            display: inline-block;
        }

        .international-market-container.inter-tableContentBase .table-body td .symbolName {
            width: 80%;
            margin-left: 10px;
        }

        .international-market-container.inter-tableContentBase .fontSize14 {
            font-size: 14px;
        }

        .international-market-container.inter-tableContent {
            width: 1180px;
            margin: 0 auto 0 auto;
        }

    </style>
    <div class="market-title-box c-5-bd c-2-bg">
        <ul class="market-title f-2-cl">
        </ul>
        <div class="search c-4-bg">
            <i class="icon-serach f-2-cl">
                <svg class="icon icon-14" aria-hidden="true">
                    <use xlink:href="#icon-a_12"></use>
                </svg>
            </i>
            <input
                    class="search-input f-2-cl"
                    type="text"
                    placeholder="{{locale.home.searchSy}}"/>
        </div>
    </div>
    <div class="home-table">
        <ul class="home-thead c-5-bd">
        </ul>
        <div class="home-loading">
            <div class="loading-page">
                {% import "../loading/loading.njk" as loading %}
                {{ loading.boxStyle(size=45) }}
            </div>
        </div>
        <div>
            <div class="home-nodata" style="display: none">
                <svg class="icon icon-50" aria-hidden="true">
                    <use xlink:href="#icon-g_2"></use>
                </svg>
                <p>
                    <!-- 暂无数据 -->
                    {{locale.common.notData}}
                </p>
            </div>
            <div class="home-tbody-box c-2-bg" style="display: none">
            </div>
        </div>
    </div>
    <script>
        (function () {
            const {myStorage, getScript} = window.BlockChainUtils;
            const extendPath = "{{sourceMap.marketExtend}}";
            const websocketPath = "{{sourceMap.webSocket}}";
            if (extendPath && !window.MarketExtend) {
                getScript(`${window.staticDomain}/home/static/${extendPath}`).then(() => {
                    class Market extends window.MarketExtend {
                        constructor() {
                            super();
                            this.$tableContent = document.querySelector('#table-content');
                            this.$marketTitle = this.$tableContent.querySelector('.market-title');
                            this.$homeThead = this.$tableContent.querySelector('.home-thead');
                            this.$homeLoading = this.$tableContent.querySelector('.home-loading');
                            this.$homeNodata = this.$tableContent.querySelector('.home-nodata');
                            this.$homeTbody = this.$tableContent.querySelector('.home-tbody-box');
                            this.$searchInput = this.$tableContent.querySelector('.search-input');
                            this.optional_symbol_server_open = Number('{{switch.optional_symbol_server_open}}');
                            this.isLogin = window.isLogin;
                            this.hoverClass = 'a-12-bd f-1-cl';
                            this.tableListOverClass = 'c-3-bg f-1-cl'
                            this.tableListCl = 'f-1-cl';
                            this.isScroll = true;
                            this.locale = {
                                marketSet: "{{locale.home.marketSet}}",
                                halving: "{{locale.trade.halving}}",
                                maincon: '{{locale.trade.maincon}}',
                                newcon: '{{locale.trade.newcon}}',
                                seecon: '{{locale.trade.seecon}}',
                                unseal: '{{locale.trade.unseal}}',
                            };
                            this.tableBg = 'f-1-cl c-2-bg';
                            this.tableDataList = {};
                            this.listfilter = null;
                            this.symbolAll = [];
                            this.marketDataList_bar = [];
                            this.MainAreaData = [];
                            this.CreateAreaData = [];
                            this.SeeAreaData = [];
                            this.hideAreaData = [];
                            this.unsealAreaData = [];
                            this.halveAreaData = [];
                            this.MainAreaFilter = []; // 主区币种
                            this.CreateAreaFilter = []; // 创新区币种
                            this.SeeAreaFilter = []; // 观察区币种
                            this.hideAreaFilter = []; // 隐藏区币种,
                            this.unsealAreaFilter = []; // 解封区币种
                            this.halveAreaFilter = []; // 减半区币种
                            this.createIng = false;
                            this.market = [];
                            this.setMyMarketSwitch = true;
                            this.tableTreeData = null;
                            this.rebuild = false;
                            this.tableTree = null;
                            this.firstLoad = false;
                            this.sortValue = 'sort';
                            this.sortSell = false;
                            this.marketCurrent = myStorage.get('homeMarkTitle');
                            this.columns = [
                                {
                                    title: '{{locale.home.market}}', // 市场
                                    key: 'sort',
                                    sortable: true,
                                },
                                {
                                    title: '{{locale.home.close}}', // 最新价
                                    key: 'closes',
                                    sortable: true,
                                },
                                {
                                    title: '{{locale.home.applies}}', // 涨跌幅
                                    key: 'roses',
                                    sortable: true,
                                },
                                {
                                    title: '{{locale.home.height}}', // 最高价
                                },
                                {
                                    title: '{{locale.home.low}}', // 最低价
                                },
                                {
                                    title: '{{locale.home.H_volume}}', // 24H成交量
                                },
                                {
                                    title: '{{locale.home.H_turnover}}', // 24小时成交额
                                },
                            ];
                            window.emitter.on('MARKET-DATA', (data, init) => {
                                if (init) {
                                    this.rebuild = true;
                                }
                                this.initTable(data);
                            });

                            window.emitter.on('send_market', (data) => {
                                this.init(data);
                            });
                        }
                    }

                    if (websocketPath && !window.selfSocket) {
                        getScript(`${window.staticDomain}/home/static/${websocketPath}`);
                    }
                    new Market();
                });
            }
            if (websocketPath && !window.selfSocket) {
                window.selfSocket = true;
                getScript(`${window.staticDomain}/home/static/${websocketPath}`);
            }
        })();

    </script>
</div>
