    <div class="market-table inter-tableContentBase">
        <div class="lxg-title-block"></div>
        <div class="home-table market-table">
            <ul class="home-thead recommend-item">
            </ul>
            <div class="home-loading">
                <div class="loading-page">
                    {% import "../loading/loading.njk" as loading %}
                    {{ loading.boxStyle(size=45) }}
                </div>
            </div>
            <div>
                <div class="home-nodata" style="display: none">
                    <svg class="icon icon-50" aria-hidden="true">
                        <use xlink:href="#icon-g_2"></use>
                    </svg>
                    <p>
                        <!-- 暂无数据 -->
                        {{locale.common.notData}}
                    </p>
                </div>
                <div class="home-tbody-box recommend-item" style="display: none">
                </div>
            </div>
        </div>
    </div>
    <script>
        (function () {
            const {myStorage, getScript} = window.BlockChainUtils;
            const extendPath = "{{sourceMap.lxgMarketExtend}}";
            const websocketPath = "{{sourceMap.webSocket}}";
            if (!window.echarts) {
                getScript("{{sourceMap.staticDomain}}/home/static/js/echarts.min.js").then(() => {
                    if (extendPath && !window.MarketExtend) {
                        getScript(`${window.staticDomain}/home/static/${extendPath}`).then(() => {
                            class Market extends window.MarketExtend {
                                constructor() {
                                    super();
                                    this.headerSymbol = "{{headerSymbol}}".split(',');
                                    this.headerMap = {};
                                    if (this.headerSymbol.length) {
                                        this.headerSymbol.forEach((item) => {
                                            this.headerMap[item] = true;
                                        });
                                    }
                                    this.$tableContent = document.querySelector('#table-content');
                                    this.$lxgTitle = this.$tableContent.querySelector('.lxg-title-block');
                                    this.$marketTitle = this.$tableContent.querySelector('.market-title');
                                    this.$homeThead = this.$tableContent.querySelector('.home-thead');
                                    this.$homeLoading = this.$tableContent.querySelector('.home-loading');
                                    this.$homeNodata = this.$tableContent.querySelector('.home-nodata');
                                    this.$homeTbody = this.$tableContent.querySelector('.home-tbody-box');
                                    this.$searchInput = this.$tableContent.querySelector('.search-input');
                                    this.optional_symbol_server_open = Number('{{switch.optional_symbol_server_open}}');
                                    this.hoverClass = 'c-8-bg u-16-cl';
                                    this.tableListOverClass = 'hoverClass';
                                    this.liColor = '';
                                    this.liBorder = '';
                                    this.isLogin = window.isLogin;
                                    this.filId = 0;
                                    this.locale = {
                                        marketSet: "{{locale.home.marketSet}}",
                                        halving: "{{locale.trade.halving}}",
                                        maincon: '{{locale.trade.maincon}}',
                                        newcon: '{{locale.trade.newcon}}',
                                        seecon: '{{locale.trade.seecon}}',
                                        unseal: '{{locale.trade.unseal}}',
                                        hot: '{{locale.home.hot}}',
                                        all: '{{locale.home.allSymbol}}',
                                    };
                                    // this.tableBg = 'c-2-bg';
                                    this.tableDataList = {};
                                    this.listfilter = null;
                                    this.symbolAll = [];
                                    this.marketDataList_bar = [];
                                    this.MainAreaData = [];
                                    this.CreateAreaData = [];
                                    this.SeeAreaData = [];
                                    this.hideAreaData = [];
                                    this.unsealAreaData = [];
                                    this.halveAreaData = [];
                                    this.MainAreaFilter = []; // 主区币种
                                    this.CreateAreaFilter = []; // 创新区币种
                                    this.SeeAreaFilter = []; // 观察区币种
                                    this.hideAreaFilter = []; // 隐藏区币种,
                                    this.unsealAreaFilter = []; // 解封区币种
                                    this.halveAreaFilter = []; // 减半区币种
                                    this.createIng = false;
                                    this.lxg = true;
                                    this.klineColor = '#9CA1C2';
                                    this.myEcharts = {};
                                    this.market = [];
                                    this.setMyMarketSwitch = true;
                                    this.tableTreeData = null;
                                    this.rebuild = false;
                                    this.tableTree = null;
                                    this.firstLoad = false;
                                    this.sortValue = 'sort';
                                    this.sortSell = false;
                                    this.marketCurrent = 'USDT';
                                    this.allKeyLine = {};
                                    this.lineWidth = 1;
                                    this.missKline = [];
                                    this.columns = [
                                        {
                                            title: '{{locale.home.market}}', // 市场
                                            key: 'sort',
                                            sortable: true,
                                        },
                                        {
                                            title: '{{locale.home.close}}', // 最新价
                                            key: 'closes',
                                            sortable: true,
                                        },
                                        {
                                            title: '{{locale.home.applies}}', // 涨跌幅
                                            key: 'roses',
                                            sortable: true,
                                        },
                                        {
                                            title: '{{locale.home.Kline}}'
                                        },
                                        {
                                            title: '{{locale.home.height}}', // 最高价
                                        },
                                        {
                                            title: '{{locale.home.low}}', // 最低价
                                        },
                                        {
                                            title: '{{locale.home.H_volume}}', // 24H成交量
                                        },
                                        {
                                            title: '{{locale.home.H_turnover}}', // 24小时成交额
                                        },
                                    ];
                                    this.hotColumns = [
                                        {
                                            title: '#'
                                        },
                                        {
                                            title: '{{locale.home.market}}', // 市场
                                        },
                                        {
                                            title: '{{locale.home.close}}', // 最新价
                                        },
                                        {
                                            title: '{{locale.home.applies}}', // 涨跌幅
                                        },
                                        {
                                            title: '{{locale.home.Kline}}'
                                        },
                                        {
                                            title: '{{locale.home.height}}', // 最高价
                                        },
                                        {
                                            title: '{{locale.home.low}}', // 最低价
                                        },
                                        {
                                            title: '{{locale.home.H_volume}}', // 24H成交量
                                        },
                                        {
                                            title: '{{locale.home.H_turnover}}', // 24小时成交额
                                        },
                                    ];
                                    if (this.lxg) {
                                        myStorage.set('homeMarkTitle', 'USDT');
                                    }

                                    window.emitter.on('RECOMMEEND_DATA', (data) => {
                                        this.recommendData = data.recommendDataList;
                                        if (this.filId === 'hot'){
                                            this.changeHotTable(this.recommendData);
                                        }
                                    });
                                    window.emitter.on('MARKET-DATA', (data, init) => {
                                        if (init) {
                                            this.rebuild = true;
                                        }
                                        this.initTable(data);
                                    });

                                    window.emitter.on('send_market', (data) => {
                                        this.init(data);
                                    });
                                    window.emitter.on('ALL_RECOMMEEND_KLINE_DATA', (data) => {
                                        const coin = Object.keys(data)[0];
                                        this.allKeyLine[coin] = data[coin];
                                        this.renderAllkeyLine(coin);
                                    });
                                    window.emitter.on('RECOMMEEND_KLINE_DATA', (data) => {
                                        const coin = Object.keys(data)[0];
                                        this.allKeyLine[coin] = data[coin];
                                        if (this.filId === 'hot'){
                                            this.setKLineData(coin);
                                        }
                                    });
                                }
                            }

                            if (websocketPath && !window.selfSocket) {
                                getScript(`${window.staticDomain}/home/static/${websocketPath}`);
                            }
                            new Market();
                        });
                    }
                    if (websocketPath && !window.selfSocket) {
                        window.selfSocket = true;
                        getScript(`${window.staticDomain}/home/static/${websocketPath}`);
                    }
                });
            }
        })();

    </script>
</div>
